<form style="border:1px solid black;width:30%;height:30%;margin-left:34%;margin-top:5%;padding:20px;background-color:blanchedalmond;font-size:15px; text-align:center" method="post">
    <input hidden type"text" name="UserID" value=@ViewData["UserID"] />
    <label>Schedule Id</label>
    <br />
    <input type="text" name="schId" value=@Model readonly />
    <br />
    <label>First Name</label>
    <br />
    <input type="text" name="Firstname" placeholder="FirstName" />
    <br />
    <label>Last Name</label>
    <br />
    <input type="text" name="Secondname" placeholder="LastName" />
    <br />
    <label>Email</label>
    <br />
    <input type="email" name="Useremail" placeholder="Email" />
    <br />
    <label>Mobile</label>
    <br />
    <input type="text" name="Mobile" placeholder="Mobile" />
    <br />
    <label>Address</label>
    <br />
    <input type="text" name="Address" placeholder="Address" />
    <br />
    <label>Number of Passengers</label>
    <br />
    <input type="number" name="nPassengers" placeholder="Total Passengers" />
    <br />
    <label>Id Proof</label>
    <br />
    <input type="text" name="Id" placeholder="Name-IdNum" />
    <br />
    <br />
    <input type="submit" style="background-color:darkred;color:white" />

</form>
